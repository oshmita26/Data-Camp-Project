        ? 'https://www.datacamp.com'
        : 'https://www.datacamp-staging.com';

      window.snowplow('newTracker', 'co', snowplowCollectorUrl, {
        appId: 'mfe-composer-atlas',
        contexts: {
          performanceTiming: true,
        },
        discoverRootDomain: true,
        postPath: '/spevent',
      });

      var snowplowUserId = Number(window.dcmfe.env.userId) || 0;

      function buildUserContext () {
        return {
          schema: 'iglu:com.datacamp/user/jsonschema/1-0-0',
          data: { userId: snowplowUserId.toString() },
        };
      }

      function buildContexts () {
        var contexts = [];

        if (snowplowUserId > 0) {
          contexts.push(buildUserContext());
        }

        return contexts;
      };

      var snowplowContexts = buildContexts();

      if (snowplowUserId > 0) {
        window.snowplow('setUserId', snowplowUserId.toString());
      }

      function snowplowTrackPageView () {
        window.snowplow('trackPageView', { context: snowplowContexts });
      }

      window.snowplow('enableActivityTracking', {
        minimumVisitLength: 10,
        heartbeatDelay: 10,
      });

