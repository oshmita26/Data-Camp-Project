      // NOTE: this is single-spa patched popstate not the default browser implementation!
      window.addEventListener('popstate', function () {
        snowplowTrackPageView();
      });

      function getContexts () {
        return buildContexts();
      }

      function trackLinkClick (args) {
        window.snowplow(
          'trackLinkClick',
          {
            context: args.context || snowplowContexts,
            elementClasses: args.elementClasses,
            elementContent: args.elementContent,
            elementId: args.elementId,
            elementTarget: args.elementTarget,
            targetUrl: args.targetUrl,
          },
        );
      }

      function autoTrackClicks (event) {
        var selectors = 'a,button,label,tr';
        var doNotTrack = {
          shouldTrack: false,
        };

        var target = event.target;
        if (target === null) {
          return doNotTrack;
        }

        var correspondingUseElement = target.correspondingUseElement;
        if (correspondingUseElement) {
          target = correspondingUseElement;
        }

        var closestIsFunc = target && typeof target.closest === 'function';
        var closestElement = closestIsFunc && target.closest(selectors);

        if (closestElement && closestElement.hasAttribute('data-trackid')) {
          var dataTrackId = closestElement.getAttribute('data-trackid');

          var classes = closestElement.className
            ? closestElement.className.match(/\S+/g) || []
            : [];

          var elementTarget = closestElement.hasAttribute('data-tracktarget')
            ? closestElement.getAttribute('data-tracktarget')
            : closestElement.getAttribute('target');

          var targetUrl;

          if (closestElement.hasAttribute('data-trackurl')) {
            targetUrl = closestElement.getAttribute('data-trackurl');
          } else if (
            closestElement.hasAttribute('href') &&
            closestElement.getAttribute('href') &&
            closestElement.getAttribute('href').length > 0 &&
            closestElement.getAttribute('href') !== '#'
          ) {
            targetUrl = closestElement.getAttribute('href');
          } else {
            targetUrl = window.location.href;
          }

          trackLinkClick({
            context: snowplowContexts,
            elementClasses: classes,
            elementContent: closestElement.innerHTML,
            elementId: dataTrackId,
            elementTarget: elementTarget,
            targetUrl: targetUrl,
          })
        }

        return doNotTrack;
      }

      function trackStructEvent (args) {
        window.snowplow('trackStructEvent',
          {
            context: args.context || snowplowContexts,
            action: args.action,
            category: args.category,
            label: args.label,
            property: args.property,
            value: args.value,
          },
        );
      }

      window.addEventListener('click', autoTrackClicks, false);

      window.dcmfeSnowplow = function (method, args) {
        var methods = ['getContexts', 'trackLinkClick', 'trackStructEvent'];

        if (window.snowplow && methods.indexOf(method) > -1) {
          if (method === 'getContexts') {
            return getContexts();
          } else if (method === 'trackLinkClick') {
            trackLinkClick(args);
          } else if (method === 'trackStructEvent') {
            trackStructEvent(args);
          }
