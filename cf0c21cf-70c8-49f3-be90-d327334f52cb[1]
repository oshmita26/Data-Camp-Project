    window.dcmfe.installPlugin({
      name: 'sentry',
      boot: function (self, ctx) {
        if (window.Sentry === undefined) return;

        window.Sentry.onLoad(function() {
          window.Sentry.init({
            attachStacktrace: true,
            beforeSend: function (event, hint) {
              ctx.log('beforeSend', event, hint);

              if (event.message && event.message === 'DCMFE_SENTRY_ERROR_ROUTING') {
                return event;
              }

              try {
                if (!event.exception.values[0].stacktrace) {
                  const tags = event.tags ? events.tags : [];
                  tags.push('missing-stacktrace');
                  event.tags = tags;
                  return event;
                }
                var frames = event.exception.values[0].stacktrace.frames;
                var errorSrc = frames[frames.length-1].filename;

                if (errorSrc.match(/sentry-cdn/) && frames.length > 1) {
                  // special case for unhandled errors caught by the way sentry
                  // replaces the window.onunhandledrejection method. This will
                  // place the sentry bundle first in stacktrace, but we want
                  // the next filename in the stack.
                  errorSrc = frames[frames.length-2].filename;
                }

                var match = errorSrc.match(/(mfe-(?:app|parcel)-[a-z-]+)/);
                if (match && match[1]) {
                  var name = match[1];
                  var client = self._private.getClient(name);
                  ctx.log('error from ' + name);

                  if (client) {
                    ctx.log('Do not send event to mfe-composer-atlas');

                    if (
                      window.dcmfe.env.appEnv === 'production' &&
                      !client.getOptions().enabled
                    ) {
                      ctx.log('send client disabled error to mfe-composer-atlas');
                      window.Sentry.captureMessage('DCMFE_SENTRY_CLIENT_DISABLED', 'debug', hint, {
                        fingerprint: ['DCMFE_SENTRY_CLIENT_DISABLED'],
                      });
                    } else {
                      client.captureEvent(event);
                    }

                    return null;
                  }
                }
              } catch (error) {
                ctx.log('send error routing error to mfe-composer-atlas', error);
                window.Sentry.captureException(error, {
                  fingerprint: ['DCMFE_SENTRY_ERROR_ROUTING'],
                  extra: {
                    event
                  }
                })
                return null;
              };

              ctx.log('Send event to mfe-composer-atlas');
              return event;
            },
            dsn: 'https://5f05911a9d5b46a7a1d6ebcabc222ad9@o61655.ingest.sentry.io/5682618',
            environment: window.dcmfe.env.appEnv,
            normalizeDepth: 6,
          });

          window.Sentry.setUser({ id: '' });
        });

        window.Sentry.forceLoad();
      },
      private: function(_self, ctx) {
        var clients = {};

        return {
          setClient: function (name, client) {
            ctx.log('setClient ' + name);
            if (clients[name]) {
              ctx.log(name + ' client is already registered');
              return;
            }
            clients[name] = client;
          },
          getClient: function (name) {
            ctx.log('getClient ' + name);
            if (!clients[name]) {
              ctx.log(name + ' client is not registered');
              return;
            }
            return clients[name];
          },
        };
      },
      public: function (self, ctx) {
        return {
          registerClient: function (name, client) {
            ctx.log('registerClient');
            self._private.setClient(name, client);
          },
        };
      },
